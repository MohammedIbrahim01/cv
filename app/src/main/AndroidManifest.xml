<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.x.cv">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Welcome"
            android:label="Welcome" />

        <activity
            android:name=".education_activity.EducationActivity"
            android:label="Education History" />
        <activity android:name=".previous_work.PreviousWorkActivity" />

        <activity android:name=".memories.MemoriesActivity" />

        <activity
            android:name=".education_activity.CertificateActivity"
            android:label="Touch out side to close"
            android:theme="@style/AppTheme.certificateStyle" />
        <activity android:name=".previous_work.AddPreviousWorkActivity" />

        <activity
            android:name=".activities.Activities"
            android:label="Activities"
            android:theme="@style/AppTheme.activities" />
    </application>

</manifest>